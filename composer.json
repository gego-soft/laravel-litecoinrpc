{
    "name": "gego-soft/laravel-litecoinrpc",
    "type": "library",
    "description": "Litecoin JSON-RPC Service Provider for Laravel",
    "keywords": ["laravel","litecoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/gego-soft/laravel-litecoinrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "GegoSoft",
            "email": "info@gegosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": ">=4.0.0",
        "majestic/php-litecoinrpc": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",  
        "orchestra/testbench": "3.x",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4" : {
            "Gegosoft\\Litecoin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gegosoft\\Litecoin\\Providers\\ServiceProvider"
            ]
        }
    }
}
